{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": true,
    "sourceMap": true,
    "outDir": "./build/",
    "target": "es5",
    "strict": true,
    "baseUrl": "src/",
    "preserveConstEnums": true
  },
  "include": ["src/**/*"],
  "exclude": ["src/components/__tests__/"]
}
